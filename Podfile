source 'https://github.com/CocoaPods/Specs.git'

# the platform for which a static library should be built against (if say we were installing one)
platform :ios, '10.0'

# tells CocoaPods we want to use Frameworks instead of Static Libraries
# because Swift/Xcode (prior to Xcode 9, beta 4 anyways) do not support them.
use_frameworks!

inhibit_all_warnings!

target 'Lastma' do
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  pod 'SnapKit'
end